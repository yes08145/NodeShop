           <label for="exampleFormControlSelect1"><%= mesdata[0].u_question %> </label>
         </div>
       </div>
       <div class="form-group">
         <label for="exampleInputEmail1">답변 제목 :</label>
         <label for="exampleFormControlSelect1"><%= mesdata[0].a_title %> </label>
       </div>
       <div class="form-group">
         <label for="exampleInputEmail1">답변 내용 :</label>
         <div>
           <label for="exampleFormControlSelect1"><%= mesdata[0].a_answer %> </label>
         </div>
        </div>
         <div class="form-group">
           <label for="exampleFormControlSelect1"><%= mesdata[0].a_status %> </label>
         </div>
         <hr>
         <button type="button" class="btn btn-secondary" onclick="back()">뒤로가기</button>
     </div>
     </div>
    </div><!-- /.container -->

